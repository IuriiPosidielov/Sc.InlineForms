<?xml version="1.0" encoding="utf-8" ?>
<control xmlns:def="Definition" xmlns="http://schemas.sitecore.net/Visual-Studio-Intellisense">
  <RichText.InsertForm>
    <FormDialog Header="Insert Form" Text="Pick a Form you want to insert." OKButton="Ok">
      <Stylesheet Src="MediaBrowser.css" DeviceDependant="true" />
      <script Type="text/javascript" Language="javascript" Src="/sitecore/shell/Controls/Rich Text Editor/InsertForm/InsertForm.js">.</script>
      <CodeBeside Type="SmartSitecore.Foundation.InlineFormsRichText.Dialogs.InsertFormDialog, SmartSitecore.Foundation.InlineFormsRichText"/>
      <DataContext ID="FormId" />
      <GridPanel Width="100%" Height="70%" Columns="3" GridPanel.Height="100%" Class="scMediaGridPanel">
        <Scrollbox ID="FormListView" Width="100%"  GridPanel.Width="100%" GridPanel.VAlign="top" />
      </GridPanel>
    </FormDialog>
  </RichText.InsertForm>
</control>
